# MySQL Database Configuration
MYSQL_ROOT_PASSWORD=your_secure_root_password_here
MYSQL_DATABASE=dagbok
MYSQL_USER=dagbok_user
MYSQL_PASSWORD=your_secure_password_here

# JWT Configuration
JWT_SECRET=your_jwt_secret_key_here

# CORS Configuration
CORS_ALLOWED_ORIGINS=http://localhost:3000

# OpenRouter API Configuration (optional)
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Next.js API URL Configuration
# For production: use the public URL that clients will access (e.g., http://yourdomain.com)
# For local development: use http://localhost:8080
NEXT_PUBLIC_API_URL=http://localhost:8081

# Internal API URL for server-side requests (container-to-container communication)
# This should be the internal Docker network URL
INTERNAL_API_URL=http://backend:8081

SPRING_JPA_SHOW_SQL=true
